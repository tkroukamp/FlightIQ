                "value": "module.exports = a = async ({params}) => {\n    console.log('params: ', params)\n\n    const parsedInput = typeof params.input === 'string' ? JSON.parse(params.input) : params.input;\n\n    try {\n        // Step 1: Validate input structure\n        if (!parsedInput || typeof parsedInput !== 'object') {\n            throw new Error(\"Invalid input: Expected an object.\");\n        }\n\n        // Step 2: Extract and parse the rawResponse if needed\n        let parsedPayload;\n        if (parsedInput.payload && typeof parsedInput.payload.body === 'string') {\n            parsedPayload = JSON.parse(parsedInput.payload.body);\n        } else {\n            throw new Error(\"Invalid payload structure or missing 'body' field.\");\n        }\n\n        // Step 3: Validate that parsedPayload is an array\n        if (!Array.isArray(parsedPayload)) {\n            throw new Error(\"Parsed body is not an array.\");\n        }\n\n        // Step 4: Extract pingUser objects\n        const users = parsedPayload\n            .filter(entry => entry.success && entry.pingUser)\n            .map(entry => entry.pingUser);\n\n        return { users };\n    } catch (error) {\n        console.error(\"Error extracting users:\", error.message);\n        throw new Error(\"Failed to parse input and extract users: \" + error.message);\n    }\n}\n"
